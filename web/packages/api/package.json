{
  "name": "@hss-science/api",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "gen:accounts": "swagger2openapi ./openapi/public/accounts/v1/accounts.swagger.json -o ./openapi/public/accounts/v1/accounts.openapi.json && openapi-typescript ./openapi/public/accounts/v1/accounts.openapi.json -o ./src/generated/accounts.ts",
    "gen:drive": "swagger2openapi ./openapi/public/drive/v1/drive.swagger.json -o ./openapi/public/drive/v1/drive.openapi.json && openapi-typescript ./openapi/public/drive/v1/drive.openapi.json -o ./src/generated/drive.ts",
    "gen": "pnpm gen:accounts && pnpm gen:drive"
  },
  "dependencies": {
    "openapi-fetch": "^0.15.0",
    "openapi-typescript": "^7.10.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.9",
    "swagger2openapi": "^7.0.8",
    "typescript": "^5.0.0"
  }
}