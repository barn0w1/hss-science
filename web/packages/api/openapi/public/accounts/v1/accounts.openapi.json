{
    "openapi": "3.0.0",
    "info": {
        "title": "public/accounts/v1/accounts.proto",
        "version": "version not set"
    },
    "tags": [
        {
            "name": "AccountsService"
        }
    ],
    "paths": {
        "/v1/auth/login": {
            "post": {
                "summary": "Exchange Discord OAuth Code for access tokens",
                "operationId": "AccountsService_Login",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v1LoginResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v1LoginRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "AccountsService"
                ]
            }
        },
        "/v1/auth/logout": {
            "post": {
                "summary": "Invalidate Refresh Token (Logout)",
                "operationId": "AccountsService_Logout",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v1LogoutResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v1LogoutRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "AccountsService"
                ]
            }
        },
        "/v1/auth/refresh": {
            "post": {
                "summary": "Refresh Access Token using Refresh Token",
                "operationId": "AccountsService_RefreshToken",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v1RefreshTokenResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v1RefreshTokenRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "AccountsService"
                ]
            }
        },
        "/v1/auth/url": {
            "get": {
                "summary": "Get authentication URL for Discord OAuth2",
                "operationId": "AccountsService_GetAuthUrl",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v1GetAuthUrlResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "AccountsService"
                ]
            }
        },
        "/v1/users/me": {
            "get": {
                "summary": "Get own user information (requires Access Token)",
                "operationId": "AccountsService_GetMe",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v1User"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "AccountsService"
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "protobufAny": {
                "type": "object",
                "properties": {
                    "@type": {
                        "type": "string"
                    }
                },
                "additionalProperties": {}
            },
            "rpcStatus": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "message": {
                        "type": "string"
                    },
                    "details": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/protobufAny"
                        }
                    }
                }
            },
            "v1GetAuthUrlResponse": {
                "type": "object",
                "properties": {
                    "url": {
                        "type": "string"
                    }
                }
            },
            "v1LoginRequest": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string"
                    }
                }
            },
            "v1LoginResponse": {
                "type": "object",
                "properties": {
                    "access_token": {
                        "type": "string"
                    },
                    "refresh_token": {
                        "type": "string",
                        "title": "Cookie使用時は空文字が返る想定だが、型としては残しておく"
                    },
                    "expires_in": {
                        "type": "integer",
                        "format": "int32"
                    }
                }
            },
            "v1LogoutRequest": {
                "type": "object",
                "properties": {
                    "refresh_token": {
                        "type": "string"
                    }
                }
            },
            "v1LogoutResponse": {
                "type": "object"
            },
            "v1RefreshTokenRequest": {
                "type": "object",
                "properties": {
                    "refresh_token": {
                        "type": "string"
                    }
                }
            },
            "v1RefreshTokenResponse": {
                "type": "object",
                "properties": {
                    "access_token": {
                        "type": "string"
                    },
                    "refresh_token": {
                        "type": "string"
                    },
                    "expires_in": {
                        "type": "integer",
                        "format": "int32"
                    }
                }
            },
            "v1User": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "avatar_url": {
                        "type": "string"
                    },
                    "role": {
                        "type": "string"
                    }
                }
            }
        }
    }
}